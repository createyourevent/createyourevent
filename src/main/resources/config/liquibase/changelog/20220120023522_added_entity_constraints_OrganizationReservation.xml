<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd">
    <!--
        Added the constraints for entity OrganizationReservation.
    -->
    <changeSet id="20220120023522-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="organization_reservation"
                                 constraintName="fk_organization_reservation__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="event_id"
                                 baseTableName="organization_reservation"
                                 constraintName="fk_organization_reservation__event_id"
                                 referencedColumnNames="id"
                                 referencedTableName="event"/>

        <addForeignKeyConstraint baseColumnNames="organization_id"
                                 baseTableName="organization_reservation"
                                 constraintName="fk_organization_reservation__organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="organization"/>
    </changeSet>
</databaseChangeLog>
