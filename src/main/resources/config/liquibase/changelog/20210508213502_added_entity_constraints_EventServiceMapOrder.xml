<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity EventServiceMapOrder.
    -->
    <changeSet id="20210508213502-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="event_id"
                                 baseTableName="event_service_map_order"
                                 constraintName="fk_event_service_map_order__event_id"
                                 referencedColumnNames="id"
                                 referencedTableName="event"/>

        <addForeignKeyConstraint baseColumnNames="service_map_id"
                                 baseTableName="event_service_map_order"
                                 constraintName="fk_event_service_map_order__service_map_id"
                                 referencedColumnNames="id"
                                 referencedTableName="service_map"/>

        <addForeignKeyConstraint baseColumnNames="cart_id"
                                 baseTableName="event_service_map_order"
                                 constraintName="fk_event_service_map_order__cart_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cart"/>
    </changeSet>
</databaseChangeLog>
